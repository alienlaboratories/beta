  "scripts": {
    "//": [
      "nodemon => babel-node: runs the server with auto-rebuild (via babel) on change.",
      "start => runs the server with hot module replacement (server not updated)."
    ],
    "webpack": "webpack --watch",
    "webpack-server": "webpack --config webpack.config.js",
    "babel-node": "BABEL_DISABLE_CACHE=1 babel-node src/server/main.js",
    "nodemon": "hilite nodemon --config nodemon.json --exec npm run babel-node",
    "server": "webpack --config webpack.config.js && node dist/server.bundle.js",
    "hot": "BABEL_DISABLE_CACHE=1 BABEL_ENV=hot NODE_ENV=hot hilite babel-node src/server/main.js",
    "start": "npm run hot"
  }
