  "scripts": {
    "//": [
      "nodemon => babel-node: runs the server with auto-rebuild (via babel) on change.",
      "start => runs the server with hot module replacement (server not updated)."
    ],
    "webpack": "webpack --watch",
    "lint": "eslint -c ../../.eslintrc.yml ./src",
    "test": "jest --silent",
    "test-server": "BABEL_DISABLE_CACHE=1 NODE_ENV=testing ALIEN_CONFIG='alienlabs-testing' babel-node src/server/main.js",
    "hot": "BABEL_DISABLE_CACHE=1 BABEL_ENV=hot NODE_ENV=hot ALIEN_CONFIG='alienlabs-testing' hilite babel-node src/server/main.js",
    "server": "BABEL_DISABLE_CACHE=1 babel-node src/server/main.js",
    "start": "hilite nodemon --exec npm run server"
